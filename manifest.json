{
  "name": "Mat's bookmarks API test",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Bookmarks API test",
  "icons": {
    "16":  "icons/froggie16.png",
    "48":  "icons/froggie48.png",
    "128": "icons/froggie128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  },
  "permissions": [
    "bookmarks",
    "notifications",
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*"
  ]
}